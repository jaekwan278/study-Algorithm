우유와 요거트가 담긴 장바구니

-- 코드를 입력하세요
SELECT cart_id
FROM cart_products
WHERE name IN ('Milk', 'Yogurt')
GROUP BY cart_id
HAVING COUNT(DISTINCT name) >= 2
ORDER BY cart_id;

SELECT DISTINCT cart_id
FROM cart_products p
WHERE EXISTS(
    SELECT 1
    FROM cart_products
    WHERE p.cart_id = cart_id AND name = 'Milk'
) AND EXISTS(
    SELECT 1
    FROM cart_products
    WHERE p.cart_id = cart_id AND name = 'Yogurt'
)
ORDER BY cart_id;

